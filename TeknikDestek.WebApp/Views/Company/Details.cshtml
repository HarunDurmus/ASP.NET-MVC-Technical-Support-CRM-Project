@using TeknikDestek.WebApp
@model TeknikDestek.Entities.Company

@{
    ViewBag.Title = "Details";
}


<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h4>Kategori</h4>
        <hr />
        <dl class="dl-horizontal">
            
            <dt>
                @Html.DisplayNameFor(model => model.LocationId)
            </dt>
            
            <dt>
                @Html.DisplayNameFor(model => model.email)
            </dt>
            <dt>
            @Html.DisplayNameFor(model => model.phoneNumber)
            </dt>
        </dl>
    </div>
    <div class="col-md-8 col-md-offset-2">
        <p>
            @Bootstrap.LinkButton("/Category/Edit/" + Model.Id, "warning", string.Empty, string.Empty, "edit", " Düzenle")
            @Bootstrap.LinkButton("/Category/Index", "default", string.Empty, string.Empty, "arrow-left", " Geri Dön!")
        </p>
    </div>
</div>
